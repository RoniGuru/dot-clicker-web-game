{
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/assets/*.ts",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/assets/*.tsx",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
